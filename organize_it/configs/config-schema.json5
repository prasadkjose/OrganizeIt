{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "title": "config-schema.json5",
    "properties": {
        "source": {
            "type": "string"
        },
        "destination": {
            "type": "string"
        },
        "format": {
            "type": "object",
            "patternProperties": {
                "^[a-z0-9]+$": {
                    "type": "object",
                    "properties": {
                        "format": {
                            "type": "array"
                        },
                        "directory_name": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }   
}